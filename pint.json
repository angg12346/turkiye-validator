{
    "preset": "laravel",
    "rules": {
        "ordered_imports": {
            "sort_algorithm": "alpha",
            "imports_order": [
                "const",
                "class",
                "function"
            ]
        },
        "no_unused_imports": true,
        "not_operator_with_successor_space": false,
        "simplified_null_return": false,
        "strict_param": true,
        "array_indentation": true,
        "declare_strict_types": true
    }
}